		.header
		count 0
		sum 4
		i 8
		
		.code
		movea mar @count
		movec mbr 10
		sto
		movea mar @sum
		movec mbr 0
		sto
		movea mar @i
		movec mbr 0
		sto
start:
		movea mar @i
		// 메모리에서 i를 읽어온 것
		lda
		mover ac1 mbr
		// mbr 에 있는 것을 ac로 옮김
		movea mar @count
		lda 
		mover ac2 mbr
		subr ac1 ac2
		// ac1에서 ac2를 빼고 그 값을 ac1에 저장.
		gtj end
		movea mar @sum
		lda
		mover ac1 mbr
		movec ac2 1
		addr ac1 ac2
		mover mbr ac1
		sto
		movea mar @i
		lda
		mover ac1 mbr
		movec ac2 1
		addr ac1 ac2
		mover mbr ac1
		sto
		jump start
end:	
		halt
.end