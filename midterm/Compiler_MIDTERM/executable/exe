move		.header
		$DS 12
		count 0
		sum 4
		i 8
		
		.code
		move mar @count
		move mbr 10
		sto
		move mar @sum
		move mbr 0
		sto
		move mar @i
		move mbr 0
		sto
start:
		move mar @i // 메모리에서 i를 읽어온 것
		lda
		move ac1 mbr // mbr 에 있는 것을 ac로 옮김
		move mar @count
		lda 
		move ac2 mbr
		sub ac1 ac2 // ac1에서 ac2를 빼고 그 값을 ac1에 저장.
		gtj end
		move mar @sum
		lda
		move ac1 mbr
		move ac2 1
		add ac1 ac2
		move mar ac1
		move mbr ac1
		sto 
		jump start
end: 	halt
.end